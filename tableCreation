-- ------------------------------------------------------------------
-- company recurity reference table
-- ------------------------------------------------------------------
CREATE TABLE company (
ISIN TEXT (12)
,Symbol TEXT (25)
,Series TEXT (2)
,Company TEXT (100)
,"52WkHigh" REAL
,"52WkHighDt" TEXT
,"52WkLow" REAL
,"52WkLowDt" TEXT
,"52WkHighDays" INT
,"52WkLowDays" INT
);
-- ------------------------------------------------------------------
-- bhav Daily price stage table
-- ------------------------------------------------------------------
CREATE TABLE bhav (
Symbol TEXT (25)
,Series TEXT (2)
,Open REAL
,High REAL
,Low REAL
,Close REAL
,Last REAL
,PrevClose REAL
,TrdQty REAL
,TrdVal REAL
,TimeStamp TEXT (10)
,Trade INTEGE
,ISIN TEXT (12)
);
-- ------------------------------------------------------------------
-- cmvolt Daily volality stage table
-- ------------------------------------------------------------------
CREATE TABLE cmvolt (
date		TEXT(10)
,Symbol		TEXT(25)
,Close		REAL
,PrevClose	REAL
,LogReturn	REAL
,PrevVolatility REAL
,DayVolatility  REAL
,YearVolatility REAL
);
-- ------------------------------------------------------------------
-- mto Daily Delivery Positions stage table
-- ------------------------------------------------------------------
CREATE TABLE mto (
Type		TEXT(2)
,SrNo		INTEGER
,Symbol		TEXT(25)
,Series		TEXT(2)
,TrdQty		INTEGER
,DelQty		INTEGER
,DelPer		REAL
);
-- ------------------------------------------------------------------
-- Data Table for daily price from bhav
-- ------------------------------------------------------------------
CREATE TABLE Low (ISIN TEXT (12) );
CREATE TABLE High (ISIN TEXT (12));
CREATE TABLE PrevClose (ISIN TEXT (12));
CREATE TABLE Open (ISIN TEXT (12));
CREATE TABLE Close (ISIN TEXT (12));
CREATE TABLE Trade (ISIN TEXT (12));
-- ------------------------------------------------------------------
-- Data Table for daily Positions from mto
-- ------------------------------------------------------------------
CREATE TABLE TrdQty (ISIN TEXT (12));
CREATE TABLE DelQty (ISIN TEXT (12));
CREATE TABLE DelPer (ISIN TEXT (12));
-- ------------------------------------------------------------------
-- Data Table for daily volality from cmvolt
-- ------------------------------------------------------------------
CREATE TABLE LogReturn (ISIN TEXT (12));
CREATE TABLE PrevVolatility (ISIN TEXT (12));
CREATE TABLE DayVolatility (ISIN TEXT (12));
CREATE TABLE YearVolatility (ISIN TEXT (12));



